# -*- coding: utf-8 -*-

import requests
import pytest



querystring = {"keuword":"聚合"}
r = requests.get(url="http://v.juhe.cn/siteTools/app/BaiduKeyword/query.php",params=querystring)
print(r.text)
print(r.url)
print(r.json()["reason"])#直接加中括号取值，找不到时抛异常
print(r.json().get("reason3","没取到"))#调用get方法取值，找不到时返回设置的没取到
